{
  "manifest_version": 2,
  "name": "FTRI 10 Staff Replacer",
  "version": "1.0",
  "description": "Chrome Extension for replacing images in your current tab with the lovely faces of FTRI 10",
  "background" : {
    "scripts" : ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "main.js" ]
    }
  ],
  "browser_action": {
    "default_title": "HI JARED",
    "default_icon": "/docs/assets/images/jared.png",
    "default_popup" : "popup.html"
  },
  "web_accessible_resources": [
    "/docs/assets/images/aiden.png",
    "/docs/assets/images/annie.png",
    "/docs/assets/images/cody.png",
    "/docs/assets/images/corey.png",
    "/docs/assets/images/demi.png",
    "/docs/assets/images/jared.png",
    "/docs/assets/images/kat.png",
    "/docs/assets/images/kristin.png",
    "/docs/assets/images/laura.png",
    "/docs/assets/images/louie.png",
    "/docs/assets/images/mia.png",
    "/docs/assets/images/mike.png",
    "/docs/assets/images/peaches.png",
    "/docs/assets/images/phillip.png",
    "/docs/assets/images/samantha.png",
    "/docs/assets/images/shanda.png",
    "/docs/assets/images/simon.png",
    "/docs/assets/images/sophia.png",
    "/docs/assets/images/xochilt.png",
    "/docs/assets/images/zachary.png"
]
}